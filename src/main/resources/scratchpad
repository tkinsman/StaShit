            try {

                while (s.hasNext()) {
                    String word = s.next();
                    String alpha = alphabetize(word);
                    List<String> l = m.get(alpha);
                    if (l == null)
                        m.put(alpha, l=new ArrayList<String>());
                    l.add(word);
                }
            } catch (IOException e) {
                System.err.println(e);
                System.exit(1);
            }


